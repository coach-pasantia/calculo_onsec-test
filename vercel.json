{
  "version": 2,
  "builds": [
      {
          "src": "back-end/index.js",
          "use": "@now/node"
      }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/back-end/index.js"
    },
    {
      "src": "/api/v1/user(.*)",
      "dest": "/back-end/index.js"
    },
    {
      "src": "/api/v1/product(.*)",
      "dest": "/back-end/index.js"
    },
    {
      "src": "/api/v1/empleados(.*)",
      "dest": "/back-end/index.js"
    }
  ]
}
